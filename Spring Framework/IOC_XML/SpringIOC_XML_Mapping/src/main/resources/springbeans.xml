<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE beans PUBLIC "-//SPRING//DTD BEAN 2.0//EN"
        "http://www.springframework.org/dtd/spring-beans-2.0.dtd">

<beans>

	<bean id="e1" class="com.entity.Employee">
		<property name="eid" value="1"></property>
		<property name="ename" value="Ravi"></property>
		<property name="salary" value="3LPA"></property>
		<property name="con" ref="c"></property>
	</bean>

	<bean id="c" class="com.entity.Country">
		<property name="cid" value="+91"></property>
		<property name="cname" value="India"></property>
	</bean>

</beans>